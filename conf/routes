# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.index()
POST    /generate                   controllers.Application.generate()
GET     /serial                     controllers.Application.serial()
POST    /serial/write               controllers.Application.serialWrite()
GET     /serial/read                controllers.Application.serialRead()
GET     /:id                        controllers.Application.generation(id: Long)
POST    /setFitness                 controllers.Application.setFitness()

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
